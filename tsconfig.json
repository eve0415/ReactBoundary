{
	"compilerOptions": {
		"module": "Node16",
		"moduleResolution": "Node16",
		"target": "ES2024",
		"lib": [
			"ES2024",
			"webworker"
		],
		"types": [
				"vscode"
		],
		"outDir": "./out",
		"strict": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"declaration": true,
		"stripInternal": true,
		"sourceMap": true,
		"declarationMap": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false
	},
	"exclude": ["src/test/example"]
}
