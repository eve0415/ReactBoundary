{
  "name": "react-boundary-visualizer",
  "displayName": "React Boundary Visualizer",
  "description": "Visualize Client Component boundaries so you understand what code ships to the client.",
  "version": "0.0.7",
  "license": "MIT",
  "author": "eve0415 <40632436+eve0415@users.noreply.github.com> (https://github.com/eve0415/)",
  "publisher": "eve0415",
  "homepage": "https://github.com/eve0415/ReactBoundaryVisualizer",
  "repository": {
    "type": "git",
    "url": "https://github.com/eve0415/ReactBoundaryVisualizer"
  },
  "bugs": {
    "url": "https://github.com/eve0415/ReactBoundaryVisualizer/issues"
  },
  "engines": {
    "vscode": "^1.90.0",
    "node": ">=24.10.0",
    "pnpm": ">=10.20.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "React",
    "RSC",
    "Client Components"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./dist/extension.js",
  "contributes": {},
  "scripts": {
    "generate": "wit2ts --outDir ./src ./wit",
    "build": "npm-run-all build:*",
    "build:cargo": "cargo build --target wasm32-unknown-unknown",
    "build:esbuild": "node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc -p . --watch",
    "tsc": "tsc -p tsconfig.json & tsc -p tsconfig.test.json & tsc -p tsconfig.react.json",
    "lint": "oxlint --type-aware --fix --fix-suggestions --fix-dangerously & oxfmt",
    "test": "npm-run-all build test:*",
    "test:vscode": "tsc -p tsconfig.test.json && xvfb-run -a vscode-test --coverage --coverage-reporter=lcov --coverage-reporter=json",
    "test:rust": "cargo test",
    "package": "vsce package"
  },
  "packageManager": "pnpm@10.20.0",
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/vscode": "1.90.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.2",
    "@vscode/wasm-component-model": "^1.0.2",
    "esbuild": "^0.25.11",
    "npm-run-all": "^4.1.5",
    "ovsx": "^0.10.6",
    "oxfmt": "^0.9.0",
    "oxlint": "^1.25.0",
    "oxlint-tsgolint": "^0.4.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.9.3"
  }
}
